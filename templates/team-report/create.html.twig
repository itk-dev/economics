{% extends 'base.html.twig' %}

{% block title %}{{ 'reports.team-title'|trans }}{% endblock %}

{% block content %}
    <h1 class="page-title">{{ 'reports.team-title'|trans }}</h1>

    {{ form_start(form) }}

    {{ form_row(form.dateFrom) }}
    {{ form_row(form.dateTo) }}

    {{ form_row(form.view, {'full_name': 'view'}) }}

    <button class="button mt-3 mb-3">{{ 'view.action_next'|trans }}</button>
    {{ form_end(form) }}
{% endblock %}
