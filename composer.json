{
    "name": "itk-dev/economics",
    "description": "ITKDev economics with jira integration",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.13",
        "gedmo/doctrine-extensions": "^3.9",
        "itk-dev/openid-connect-bundle": "^3.0",
        "knplabs/knp-paginator-bundle": "^6.1",
        "mpdf/mpdf": "^8.1.2",
        "openspout/openspout": "^4.23",
        "phpoffice/phpspreadsheet": "^1.25",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/console": "~6.4.0",
        "symfony/doctrine-messenger": "~6.4.0",
        "symfony/dotenv": "~6.4.0",
        "symfony/flex": "^2",
        "symfony/form": "~6.4.0",
        "symfony/framework-bundle": "~6.4.0",
        "symfony/http-client": "~6.4.0",
        "symfony/messenger": "~6.4.0",
        "symfony/monolog-bundle": "^3.8",
        "symfony/proxy-manager-bridge": "~6.4.0",
        "symfony/runtime": "~6.4.0",
        "symfony/security-bundle": "~6.4.0",
        "symfony/stimulus-bundle": "^2.10",
        "symfony/translation": "~6.4.0",
        "symfony/twig-bundle": "~6.4.0",
        "symfony/validator": "~6.4.0",
        "symfony/webpack-encore-bundle": "^2.0",
        "symfony/yaml": "~6.4.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.0",
        "doctrine/doctrine-fixtures-bundle": "^3.5",
        "ergebnis/composer-normalize": "^2.28",
        "friendsofphp/php-cs-fixer": "^3.13",
        "friendsoftwig/twigcs": "^6.0",
        "phpunit/phpunit": "^10.2",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/debug-bundle": "~6.4.0",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "~6.4.0",
        "vimeo/psalm": "^5.0"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "check-coding-standards/php-cs-fixer": [
            "php-cs-fixer fix --dry-run --format=checkstyle"
        ],
        "check-coding-standards/twigcs": [
            "twigcs templates"
        ],
        "coding-standards-apply": [
            "php-cs-fixer fix"
        ],
        "coding-standards-check": [
            "@check-coding-standards/php-cs-fixer",
            "@check-coding-standards/twigcs"
        ],
        "prepare-code": [
            "composer normalize",
            "@coding-standards-apply",
            "@psalm",
            "@tests"
        ],
        "psalm": [
            "./vendor/bin/psalm --no-cache"
        ],
        "tests": [
            "./vendor/bin/phpunit --stop-on-failure"
        ]
    }
}
